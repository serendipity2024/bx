    "sentry_push": "node ./scripts/sentry_setup.js",
    "deploy": "NODE_ENV=production yarn deploy:docker",
    "check-branch": "git-branch-is development",
    "cypress:open": "cypress open",
    "cypress-test": "node scripts/cypress.js",
    "cypress-sync": "node scripts/cypressSync.js"
