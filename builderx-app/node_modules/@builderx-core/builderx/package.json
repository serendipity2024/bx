    "test": "echo NO TESTS",
    "jest": "NODE_ENV=test jest --env=jsdom --bail --useStderr --verbose --silent --runInBand && cat test-passed.file",
    "jest-watch": "NODE_ENV=test node --inspect node_modules/.bin/jest --env=jsdom --bail --watchAll --useStderr --verbose --silent --runInBand",
    "prepush": "if git-branch-is -q development || git-branch-is -q master ; then yarn run jest ;fi",
    "session": "concurrently --kill-others \"cd packages/session && yarn start\" \"cd packages/server && yarn dev\"",
